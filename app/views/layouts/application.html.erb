			                <li><a href="#">Separated link</a></li>
			                <li><a href="#">One more separated link</a></li>
			              </ul>
			            </li>
		            	<li><a href="/chatroom_admin">Support Tickets</a>
		            <% else %>
			            <li><a href="/chatroom">Support</a></li>
			        <% end %>
			    <li><a href="/users/edit">User Info</a></li>
	            <li><a href="/logout">Logout</a></li>
	          </ul>
          <% end %>
        </div><!--/.nav-collapse -->
      </div>
    </nav>
    <div class="container yieldCont">
	    <%= yield %>
	</div>
  </body>
</html>
